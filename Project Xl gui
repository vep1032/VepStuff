while skillsX do wait() 
    pcall(function()
            local LP = game:GetService("Players").LocalPlayer
            local VIM = game:GetService("VirtualInputManager")
         VIM:SendKeyEvent(true, Enum.KeyCode.X, false, game)
    end )
        end
            end)
            
                        sss:Toggle("Spam V Skill",function(t)
skillsV = t 
    pcall(function()
while skillsV do wait() 
            local LP = game:GetService("Players").LocalPlayer
            local VIM = game:GetService("VirtualInputManager")
         VIM:SendKeyEvent(true, Enum.KeyCode.V, false, game)
    end 
    end)
            end)

    
    sss:Button("HideName",function()
        while wait() do 
            pcall(function()
        game.Players.LocalPlayer.Character.Head.Overhead.Enabled = false 
             end)
           end
        end)
    
      ss1:Toggle("Autofarm Evolution Stones",function(t)
    itemfa = t
        while itemfa do wait()
            pcall(function()
                for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
    if v.Name == "Evolution Stone" and v:IsA("Tool") then 
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame  = v.Handle.CFrame  
    end 
end 
end)
end
      end)
  ss1:Toggle("Autofarm FlashStep",function(t)
    itemf = t
        while itemf do wait()
            pcall(function()
                for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
    if v.Name == "FlashStep" and v:IsA("Tool") then 
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame  = v.Handle.CFrame  
    end 
end 
end)
end
    end)
  
    ss1:Toggle("Autofarm FlashStep Scroll",function(t)
    ite = t
        while ite do wait()
            pcall(function()
                for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
    if v.Name == "Flashstep scroll" and v:IsA("Tool") then 
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame  = v.Handle.CFrame  
    end 
end 
end)
end
    end)
    
      ss1:Toggle("Autofarm Sondio",function(t)
    ia = t
        while ia do wait()
            pcall(function()
                for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
    if v.Name == "Sondio" and v:IsA("Tool") then 
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame  = v.Handle.CFrame  
    end 
end 
end)
end
  end)


  ss1:Toggle("Autofarm Gems",function(t)
    itf = t
        while itf do wait()
            pcall(function()
                for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
    if v.Name == "Gem" and v:IsA("Tool") then 
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame  = v.Handle.CFrame  
    end 
end 
end)
end
  end)

ss:Button("Copy Discord",function(t)
setclipboard("https://discord.gg/xdMjJ3tgvz")
end)
